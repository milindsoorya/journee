# Prisma Accelerate connection string (used at runtime)
DATABASE_URL="prisma+postgres://accelerate.prisma/YOUR_ACCELERATE_CONNECTION"

# Direct database connection used by Prisma CLI commands like `prisma db push`
DIRECT_DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Client-side key for maps integrations (optional)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=""
